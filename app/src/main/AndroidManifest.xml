<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zedr.dhaivat.yogapedia">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/loogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Advance" android:screenOrientation="portrait"></activity>
        <activity android:name=".Intermediate" android:screenOrientation="portrait"/>
        <activity android:name=".Benefits" android:screenOrientation="portrait" />
        <activity android:name=".Beginner" android:screenOrientation="portrait" />
        <activity android:name=".MainActivity" android:screenOrientation="portrait">
        </activity>
        <activity android:name=".splash" android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>